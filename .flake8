[flake8]
ignore = E501, # line too long
        W503, # line break before binary operator
        E402, # module level import not at top of file
        D202, # No blank lines allowed after function docstring
exclude =
    .git,
    __pycache__,
    docs/source/conf.py,
    old,
    build,
    dist
max-complexity = 10
